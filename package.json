{
  "name": "ebios-rm-ai-assistant",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "dev:proxy": "node proxy-server.js",
    "dev:all": "concurrently \"npm run dev:proxy\" \"npm run dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
